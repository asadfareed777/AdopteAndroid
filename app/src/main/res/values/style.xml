        <item name="android:textSize">@dimen/_10ssp</item>
        <item name="android:layout_marginBottom">@dimen/_2ssp</item>
    </style>
    <style name="imageForwardDesign">
        <item name="android:layout_width">@dimen/_20sdp</item>
        <item name="android:layout_height">@dimen/_20sdp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_vertical</item>
