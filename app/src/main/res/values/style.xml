<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="toolbarDesign">
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:background">@drawable/db_toolbar_bg</item>
        <item name="android:elevation" tools:ignore="DuplicateDefinition" tools:targetApi="lollipop">
            @dimen/_5sdp
        </item>
    </style>

    <style name="labelDesign">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/label_color</item>
        <item name="android:textSize">@dimen/_14ssp</item>
    </style>

    <style name="labelToolbarTitleDesign">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="orderHeadingDesign">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/_13ssp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="etLabelDesign">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/label_color</item>
        <item name="android:textSize">@dimen/_10ssp</item>
        <item name="android:layout_marginBottom">@dimen/_2ssp</item>
    </style>
    <style name="storeHourLabelDesign">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
