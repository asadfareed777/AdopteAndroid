    <style name="etIconDesign">
        <item name="android:layout_width">@dimen/_20sdp</item>
        <item name="android:layout_height">@dimen/_20sdp</item>
        <item name="android:padding">@dimen/_1ssp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="profileIconDesign">
        <item name="android:layout_width">@dimen/_25sdp</item>
